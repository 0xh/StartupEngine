{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "CMS",
        "Blog",
        "Startup",
        "Marketing",
        "Laravel",
        "Framework"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "ext-gd": "*",
        "appstract/laravel-meta": "^0.2.0",
        "caffeinated/modules": "^4.2",
        "fideloper/proxy": "~3.3",
        "graham-campbell/github": "^6.1",
        "graham-campbell/markdown": "^8.0",
        "jaam/mixpanel-data-export-api": "^0.1.2",
        "lanin/laravel-api-debugger": "^3.0",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "tcg/voyager": "^1.0",
        "voyager-themes": "^0.3.0",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "orangehill/iseed": "^2.5",
        "phpunit/phpunit": "~6.0",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "app/Modules/*/composer.json"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "@php artisan command:SyncGit"
        ],
        "post-update-cmd": [
            "@php artisan command:SyncGit"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://larapack.io"
        },
        "1": {
            "type": "path",
            "url": "./laravel-comments"
        },
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
